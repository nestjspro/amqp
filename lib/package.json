{
    "name": "@nestjs.pro/amqp",
    "description": "AMQP for Nest.js like a boss. ðŸ’ª",
    "version": "0.0.30",
    "license": "MIT",
    "homepage": "https://amqp.nestjs.pro",
    "readme": "README.md",
    "types": "src/index.ts",
    "author": {
        "name": "Matthew Davis",
        "email": "matthew@matthewdavis.io",
        "url": "https://matthewdavis.io"
    },
    "repository": {
        "url": "https://github.com/nestjspro/amqp.git",
        "type": "git"
    },
    "bugs": {
        "url": "https://github.com/nestjspro/amqp/issues",
        "email": "support@nestjs.pro"
    },
    "contributors": [
        {
            "name": "Matthew Davis",
            "email": "matthew@matthewdavis.io",
            "url": "https://matthewdavis.io"
        }
    ],
    "maintainers": [
        {
            "name": "Matthew Davis",
            "email": "matthew@matthewdavis.io",
            "url": "https://matthewdavis.io"
        }
    ],
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc -w",
        "test": "jest --runInBand --detectOpenHandles --forceExit",
        "test:clearcache": "jest --clearCache",
        "test:watch": "jest --maxWorkers 1 --watchAll --detectOpenHandles --no-cahe",
        "test:coverage": "jest --runInBand --coverage --detectOpenHandles --forceExit",
        "semantic-release": "semantic-release",
        "release": "tsc && npm version patch && cp package.json dist && cd dist && npm publish"
    },
    "dependencies": {
        "amqplib": "^0.10.3",
        "chalk": "^5.2.0",
        "rxjs": "^7.8.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.4.2",
        "@commitlint/config-conventional": "^17.4.2",
        "@nestjs/common": "^9.2.1",
        "@nestjs/core": "^9.2.1",
        "@nestjs/platform-express": "^9.2.1",
        "@nestjs/testing": "^9.2.1",
        "@types/amqplib": "^0.10.1",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.11.18",
        "codecov": "^3.8.3",
        "coveralls": "^3.1.1",
        "jest": "^29.4.1",
        "semantic-release": "^20.1.0",
        "ts-jest": "^29.0.5",
        "typedoc": "^0.23.24",
        "typedoc-plugin-extras": "^2.3.2",
        "typescript": "^4.9.4"
    },
    "jest": {
        "rootDir": ".",
        "moduleFileExtensions": [
            "js",
            "ts"
        ],
        "testEnvironment": "node",
        "testRegex": "Test.ts$",
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "collectCoverage": false,
        "collectCoverageFrom": [
            "src/**/{!(ignore-coverage),}*.ts"
        ],
        "verbose": true,
        "coverageThreshold": {
            "global": {
                "statements": 90,
                "branches": 50,
                "lines": 90
            }
        },
        "coverageReporters": [
            "lcov",
            "json",
            "text"
        ],
        "cacheDirectory": ".jest",
        "coverageDirectory": "./coverage"
    },
    "keywords": [
        "amqp",
        "amqplib",
        "messaging",
        "microservices",
        "nestjs",
        "nestjs.pro",
        "queue",
        "rabbitmq"
    ]
}
