{
    "name": "@nestjs.pro/test-app",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon dist/main.js\" \"tsc -w -p tsconfig.json\" "
    },
    "dependencies": {
        "@nestjs.pro/amqp": "workspace:../lib",
        "@nestjs/common": "8.4.5",
        "@nestjs/core": "8.4.5",
        "@nestjs/testing": "8.4.5",
        "dotenv": "10.0.0",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.5.5"
    },
    "devDependencies": {
        "@types/jest": "27.5.1",
        "@types/node": "16.11.35",
        "concurrently": "6.5.1",
        "jest": "27.5.1",
        "nodemon": "2.0.16",
        "supertest": "6.2.3",
        "ts-jest": "27.1.4",
        "typescript": "4.6.4",
        "wait-on": "6.0.1"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "roots": [
            "test"
        ],
        "testRegex": "Test.ts$|.spec.ts$",
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "collectCoverageFrom": [
            "src/**/*.ts"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
